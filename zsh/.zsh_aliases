# brew
alias upgrade="sudo apt update; sudo apt upgrade"
alias remove="sudo apt remove"

# cat to bat
alias bat="batcat"
alias cat="batcat"

# ls
alias ls="exa"
alias l="ls"

alias la="ls -a"
alias ll="ls -alF"
alias lt="ls --tree"

# interactive
alias mv="mv -i"
alias rm="rm -i"

# git
alias gs="git status"
alias gcc="git commit"
alias gce="git commit --amend"
alias gco="git checkout"
alias gcl="git clone"
alias gl="git log --graph --all --oneline"

